-- Simple XOR encryption function
local function encryptDecrypt(input, key)
    local output = ""
    local keyLength = string.len(key)
    local keyIndex = 1
    for i = 1, string.len(input) do
        local char = string.sub(input, i, i)
        local keyChar = string.sub(key, keyIndex, keyIndex)
        local encryptedChar = string.char(bit.bxor(string.byte(char), string.byte(keyChar)))
        output = output .. encryptedChar
        keyIndex = keyIndex % keyLength + 1
    end
    return output
end

-- Encrypted strings
local encryptedKey = "Phantom1898"
local encryptedDiscordURL = "u`k!ujxalbhdsj.moc/moc.s'cord//:ptth'/:ZW!BTD6kvwa"

-- Decrypt encrypted strings
local key = encryptDecrypt(encryptedKey, "secretkey")
local discordURL = encryptDecrypt(encryptedDiscordURL, "secretkey")

-- Load external library
local a = loadstring(game:HttpGet('https://raw.githubusercontent.com/shlexware/Orion/main/source'))()

-- Create window
local b = a:MakeWindow({
    Name = "Phantom HUB | STRONGEST BATTLEGROUND",
    HidePremium = false,
    SaveConfig = true,
    ConfigFolder = "PhantomHubConfig",
    IntroText = "Phantom Hub"
})

local c = false

-- Key Tab for unlocking
local d = b:MakeTab({
    Name = "Key",
    Icon = "rbxassetid://4483345998",
    PremiumOnly = false
})

d:AddTextbox({
    Name = "Enter Key",
    Default = "",
    TextDisappear = true,
    Callback = function(e)
        if e == key then
            c = true
            a:MakeNotification({
                Title = "Success",
                Content = "Key is correct! Script unlocked.",
                Image = "rbxassetid://4483345998",
                Time = 5
            })
        else
            a:MakeNotification({
                Title = "Error",
                Content = "Incorrect key. Please try again.",
                Image = "rbxassetid://4483345998",
                Time = 5
            })
        end
    end
})

d:AddButton({
    Name = "Copy Discord Link",
    Callback = function()
        setclipboard(discordURL)
        a:MakeNotification({
            Title = "Copied",
            Content = "Discord link copied to clipboard.",
            Image = "rbxassetid://4483345998",
            Time = 5
        })
    end
})

-- Main Tab with unlockable buttons
local f = b:MakeTab({
    Name = "Main",
    Icon = "rbxassetid://4483345998",
    PremiumOnly = false
})

local function g(h)
    return function(...)
        if c then
            h(...)
        else
            a:MakeNotification({
                Title = "Locked",
                Content = "Please enter the correct key to unlock.",
                Image = "rbxassetid://4483345998",
                Time = 5
            })
        end
    end
end

f:AddButton({
    Name = "No/Less Stun [Click Once]",
    Callback = g(function()
        local i = 0.3
        local j = game:GetService("RunService").Heartbeat
        local k = true
        local l = game:GetService("Players").LocalPlayer
        local m = l.Character
        
        while k and j:Wait() and m and m.Parent do
            local n = m:FindFirstChildOfClass("Humanoid")
            if n and n.MoveDirection.Magnitude > 0 then
                if i and tonumber(i) then
                    m:TranslateBy(n.MoveDirection * tonumber(i))
                else
                    m:TranslateBy(n.MoveDirection)
                end
            end
        end
    end)
})

f:AddButton({
    Name = "Reach",
    Callback = g(function()
        print("Reach Value 69")
    end)
})

f:AddButton({
    Name = "Camera Lock",
    Callback = g(function()
        loadstring(game:HttpGet('https://rentry.co/forkoritothegoat/raw'))()
    end)
})

f:AddButton({
    Name = "Infinite Yield",
    Callback = g(function()
        loadstring(game:HttpGet('https://raw.githubusercontent.com/EdgeIY/infiniteyield/master/source'))()
    end)
})

f:AddButton({
    Name = "Esp",
    Callback = g(function()
        local o = game:GetService("Players")
        local p = game:GetService("RunService")

        local function q()
            local r = Instance.new("BoxHandleAdornment")
            r.Size = Vector3.new(5, 5, 2)
            r.Color3 = Color3.new(1, 0, 0)
            r.Transparency = 0.1
            r.ZIndex = 5
            return r
        end

        local function s(t, u)
            local v = t.Character
            if v and v:FindFirstChild("HumanoidRootPart") then
                u.Visible = true
                u.Adornee = v.HumanoidRootPart
                u.Parent = v.HumanoidRootPart
            else
                u.Visible = false
                u.Adornee = nil
                u.Parent = nil
            end
        end

        local function w(x)
            local y = q()
            s(x, y)

            x.CharacterAdded:Connect(function()
                s(x, y)
            end)

            x.CharacterRemoving:Connect(function()
                s(x, y)
            end)
        end

        for _, z in ipairs(o:GetPlayers()) do
            w(z)
        end

        o.PlayerAdded:Connect(function(A)
            w(A)
        end)

        p.RenderStepped:Connect(function()
            for _, B in ipairs(o:GetPlayers()) do
                if B.Character then
                    local C = B.Character:FindFirstChildOfClass("BoxHandleAdornment")
                    if C then
                        s(B, C)
                    end
                end
            end
        end)
    end)
})

