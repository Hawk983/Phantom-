-- Dummy variables and functions to further obfuscate the code
local dummy1, dummy2, dummy3 = true, 123, "obfuscate"
local function dummyFunction1()
    local x = 0
    for i = 1, 10 do
        x = x + i
    end
    return x
end
local function dummyFunction2(a, b)
    return a + b
end

local S = dummyFunction1() + dummyFunction2(dummy2, 10)
local dummy4 = "dummy" .. S
local dummy5 = function() return "obfuscated" end
local T = dummy5()

local function randomObfuscate()
    return dummy1 and dummy2 or dummy3
end

local U = randomObfuscate()

-- Further dummy structure
local function deepObfuscate()
    local nested1 = "nested"
    local function nestedObfuscate()
        local nested2 = "deeply"
        return nested1 .. nested2
    end
    return nestedObfuscate()
end

local V = deepObfuscate()

-- Random no-op code
local noop1, noop2, noop3 = false, 0, ""
for i = 1, 5 do
    noop2 = noop2 + i
    noop3 = noop3 .. tostring(i)
end

local W = noop2 .. noop3 .. (noop1 and "true" or "false")

print(W)  -- Print to further distract and obfuscate the code


