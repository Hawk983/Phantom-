local function E(F)
    if c then
        _G.antiNoBlock = F
        local G = game:GetService("RunService")
        local H
        H = G.RenderStepped:Connect(function()
            if _G.antiNoBlock == true then
                if game.Players.LocalPlayer.Character:FindFirstChild("NoBlock") and _G.antiNoBlock == true then
                    game.Players.LocalPlayer.Character.NoBlock:Destroy()
                end
            end
        end)
    else
        a:MakeNotification({
            Title = "Locked",
            Content = "Please enter the correct key to unlock Anti features.",
            Image = "rbxassetid://4483345998",
            Time = 5
        })
