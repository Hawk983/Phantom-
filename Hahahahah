end

local function I(J)
    if c then
        _G.anticounter = J
        local K = game:GetService("Players")
        local L = game:GetService("RunService")
        
        local function M(N)
            if N.Character and N.Character:FindFirstChild("Counter") then
                N.Character.Counter:Destroy()
            end
        end
        
        local function O(P)
            P.CharacterAdded:Connect(function()
                M(P)
            end)
            M(P)
        end
        
        K.PlayerAdded:Connect(O)
        for _, Q in ipairs(K:GetPlayers()) do
            O(Q)
        end
    else
        a:MakeNotification({
            Title = "Locked",
            Content = "Please enter the correct key to unlock Anti features.",
            Image = "rbxassetid://4483345998",
            Time = 5
        })
    end
end

