E:AddToggle({
    Name = "Anti NoBlock",
    Default = false,
    Callback = g(function(Z)
        _G.antiNoBlock = Z
        local aa = game:GetService("RunService")
        local ab
        ab = aa.RenderStepped:Connect(function()
            if _G.antiNoBlock == true then
                local ac = game.Players.LocalPlayer.Character
                if ac and ac:FindFirstChild("NoBlock") then
                    ac.NoBlock:Destroy()
                end
            end
        end)
    end)
})

E:AddToggle({
    Name = "Anti Counter",
    Default = false,
    Callback = g(function(ad)
        _G.anticounter = ad
        local ae = game:GetService("Players")
        local af = game:GetService("RunService")

        local function ag(ah)
            if ah.Character and ah.Character:FindFirstChild("Counter") then
                ah.Character.Counter:Destroy()
            end
        end

        local function ai(aj)
            aj.CharacterAdded:Connect(function()
                ag(aj)
            end)
            ag(aj)
        end

        ae.PlayerAdded:Connect(ai)
        for _, ak in ipairs(ae:GetPlayers()) do
            ai(ak)
        end
    end)
})

