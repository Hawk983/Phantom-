-- Anti Tab
local E = b:MakeTab({
    Name = "Anti",
    Icon = "rbxassetid://4483345998",
    PremiumOnly = false
})

E:AddToggle({
    Name = "Anti Ragdoll",
    Default = false,
    Callback = g(function(F)
        _G.antiragdoll = F
        local G = game:GetService("RunService")
        local H
        H = G.RenderStepped:Connect(function()
            if _G.antiragdoll == true then
                local I = game.Players.LocalPlayer.Character
                if I and I:FindFirstChild("RagdollSim") then
                    I.RagdollSim:Destroy()
                    if I:FindFirstChild("Ragdoll") then
                        I.Ragdoll:Destroy()
                    end
                end
            end
        end)
    end)
})

E:AddToggle({
    Name = "Anti Freeze",
    Default = false,
    Callback = g(function(J)
        _G.antifreeze = J
        local K = game:GetService("RunService")
        local L
        L = K.RenderStepped:Connect(function()
            if _G.antifreeze == true then
                local M = game.Players.LocalPlayer.Character
                if M and M:FindFirstChild("Freeze") then
                    M.Freeze:Destroy()
                end
            end
        end)
    end)
})

E:AddToggle({
    Name = "Anti Slowed",
    Default = false,
    Callback = g(function(N)
        _G.antislowed = N
        local O = game:GetService("RunService")
        local P
        P = O.RenderStepped:Connect(function()
            if _G.antislowed == true then
                local Q = game.Players.LocalPlayer.Character
