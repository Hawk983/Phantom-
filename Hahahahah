})

D:AddToggle({
    Name = "Anti NoJump",
    Default = false,
    Callback = f(function(U)
        _G.antiNoJump = U
        local V = game:GetService("RunService")
        local W
        W = V.RenderStepped:Connect(function()
            if _G.antiNoJump == true then
                local X = game.Players.LocalPlayer.Character
