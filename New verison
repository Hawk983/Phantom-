    else
        OrionLib:MakeNotification({
            Title = "Locked",
            Content = "Please enter the correct key to unlock Anti features.",
            Image = "rbxassetid://4483345998",
            Time = 5
        })
    end
end

local function toggleAntiCounter(Value)
    if isUnlocked then
        _G.anticounter = Value
        local Players = game:GetService("Players")
        local RunService = game:GetService("RunService")
        
        local function toggleAntiCounterForPlayer(player)
            if player.Character and player.Character:FindFirstChild("Counter") then
                player.Character.Counter:Destroy()
            end
        end
        
        local function onPlayerAdded(player)
            player.CharacterAdded:Connect(function()
                toggleAntiCounterForPlayer(player)
            end)
            toggleAntiCounterForPlayer(player)
        end
        
        Players.PlayerAdded:Connect(onPlayerAdded)
        for _, player in ipairs(Players:GetPlayers()) do
            onPlayerAdded(player)
        end
    else
        OrionLib:MakeNotification({
            Title = "Locked",
            Content = "Please enter the correct key to unlock Anti features.",
            Image = "rbxassetid://4483345998",
            Time = 5
        })
    end
end

AntiTab:AddToggle({
    Name = "Anti Ragdoll",
    Default = false,
    Callback = toggleAntiRagdoll
})

AntiTab:AddToggle({
    Name = "Anti Freeze",
    Default = false,
    Callback = toggleAntiFreeze
})

AntiTab:AddToggle({
    Name = "Anti Slowed",
    Default = false,
    Callback = toggleAntiSlowed
})

AntiTab:AddToggle({
    Name = "Anti StopRunning",
    Default = false,
    Callback = toggleAntiStopRunning
})

AntiTab:AddToggle({
    Name = "Anti NoJump",
    Default = false,
    Callback = toggleAntiNoJump
})

AntiTab:AddToggle({
    Name = "Anti NoBlock",
    Default = false,
    Callback = toggleAntiNoBlock
})

AntiTab:AddToggle({
    Name = "Anti Counter",
    Default = false,
    Callback = toggleAntiCounter
})

