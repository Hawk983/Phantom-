local MainTab = Window:MakeTab({
    Name = "Main",
    Icon = "rbxassetid://4483345998",
    PremiumOnly = false
})

MainTab:AddButton({
    Name = "No/Less Stun [Click Once]",
    Callback = checkUnlock(function()
        -- Insert your function here
    end)
