    Name = "Destroy UI",
    Callback = function()
        OrionLib:Destroy()
    end
})

-- Keybind to toggle UI
OrionLib:MakeTab({
    Name = "Settings",
    Icon = "rbxassetid://4483345998",
    PremiumOnly = false
}):AddKeybind({
    Name = "Toggle UI",
    Default = Enum.KeyCode.K,
    Hold = false,
    Callback = function()
        OrionLib:ToggleUI()
    end
})
