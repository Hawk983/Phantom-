local combatTab = mainWindow:MakeTab({
    Name = "Combat",
    Icon = "rbxassetid://4483345998",
    PremiumOnly = false
})

combatTab:AddButton({
    Name = "Retro Hammer (with animation)",
    Callback = ensureKeyEntered(function()
        game.Players.LocalPlayer.Character.Humanoid:LoadAnimation(game.ReplicatedStorage.Assets.Retro.Animations["Ban Hammer"]):Play()
        wait(0.9)
        game:GetService("ReplicatedStorage").RetroAbility:FireServer("Ban Hammer")
    end)
})

combatTab:AddButton({
    Name = "Null Ability",
    Callback = ensureKeyEntered(function()
        game:GetService("ReplicatedStorage").NullAbility:FireServer()
    end)
})

combatTab:AddButton({
    Name = "Rhythm Explosion",
    Callback = ensureKeyEntered(function()
        game:GetService("ReplicatedStorage").rhythmevent:FireServer("AoeExplosion", 0)
    end)
})

combatTab:AddButton({
    Name = "Rojo Attack",
    Callback = ensureKeyEntered(function()
        game:GetService("ReplicatedStorage"):WaitForChild("RojoAbility"):FireServer("Release", {game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame})
    end)
})

combatTab:AddButton({
    Name = "Retro Rocket (without animation)",
    Callback = ensureKeyEntered(function()
        game:GetService("ReplicatedStorage").RetroAbility:FireServer("Rocket Launcher")
    end)
})

combatTab:AddButton({
    Name = "Retro Hammer (without animation)",
    Callback = ensureKeyEntered(function()
        game:GetService("ReplicatedStorage").RetroAbility:FireServer("Ban Hammer")
    end)
})

combatTab:AddButton({
    Name = "Explode",
    Callback = ensureKeyEntered(function()
        local character = game.Players.LocalPlayer.Character
        game:GetService("ReplicatedStorage").RetroAbility:FireServer("Bomb")
        character.HumanoidRootPart:WaitForChild("BodyVelocity"):Destroy()
    end)
})

local animationTab = mainWindow:MakeTab({
    Name = "Animation",
    Icon = "rbxassetid://4483345998",
    PremiumOnly = false
})

animationTab:AddButton({
    Name = "Stop Animations",
    Callback = ensureKeyEntered(function()
        local tracks = game.Players.LocalPlayer.Character.Humanoid.Animator:GetPlayingAnimationTracks()
        for _, track in ipairs(tracks) do
            track:Stop()
        end
    end)
})

animationTab:AddToggle({
    Name = "Thriller",
    Default = false,
    Callback = ensureKeyEntered(function(isEnabled)
        _G.isDancing = isEnabled
        if isEnabled then
            game.Players.LocalPlayer.Character.Humanoid:WaitForChild("Animator"):LoadAnimation(game.ReplicatedStorage.AnimationPack.Thriller):Play()
        else
            local tracks = game.Players.LocalPlayer.Character.Humanoid.Animator:GetPlayingAnimationTracks()
            for _, track in ipairs(tracks) do
                track:Stop()
            end
        end
    end)
})

animationTab:AddToggle({
    Name = "Kick",
