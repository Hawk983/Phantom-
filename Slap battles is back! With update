local m = d:MakeTab({Name = "Combat",Icon = "rbxassetid://4483345998",PremiumOnly = false})

m:AddButton({Name = "Retro Hammer (with animation)",Callback = h(function()
    game.Players.LocalPlayer.Character.Humanoid:LoadAnimation(game.ReplicatedStorage.Assets.Retro.Animations["Ban Hammer"],game.Players.LocalPlayer.Character.Humanoid):Play()
    wait(0.9)
    game:GetService("ReplicatedStorage").RetroAbility:FireServer("Ban Hammer")
end)})

m:AddButton({Name = "Null Ability",Callback = h(function()
    game:GetService("ReplicatedStorage").NullAbility:FireServer()
end)})

m:AddButton({Name = "Rhythm Explosion",Callback = h(function()
    game:GetService("ReplicatedStorage").rhythmevent:FireServer("AoeExplosion",0)
end)})

m:AddButton({Name = "Rojo Attack",Callback = h(function()
    game:GetService("ReplicatedStorage"):WaitForChild("RojoAbility"):FireServer("Release",{game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame})
end)})

m:AddButton({Name = "Retro Rocket (without animation)",Callback = h(function()
    game:GetService("ReplicatedStorage").RetroAbility:FireServer("Rocket Launcher")
end)})

m:AddButton({Name = "Retro Hammer (without animation)",Callback = h(function()
    game:GetService("ReplicatedStorage").RetroAbility:FireServer("Ban Hammer")
end)})

m:AddButton({Name = "Explode",Callback = h(function()
    local n = game.Players.LocalPlayer.Character
    game:GetService("ReplicatedStorage").RetroAbility:FireServer("Bomb")
    n.HumanoidRootPart:WaitForChild("BodyVelocity"):Destroy()
end)})

local o = d:MakeTab({Name = "Animation",Icon = "rbxassetid://4483345998",PremiumOnly = false})

o:AddButton({Name = "Stop Animations",Callback = h(function()
    local p = game.Players.LocalPlayer.Character.Humanoid.Animator:GetPlayingAnimationTracks()
    for q,r in ipairs(p) do
        r:Stop()
    end
end)})

o:AddToggle({Name = "Thriller",Default = false,Callback = h(function(s)
    _G.isDancing = s
    if _G.isDancing then
        game.Players.LocalPlayer.Character.Humanoid:WaitForChild("Animator"):LoadAnimation(game.ReplicatedStorage.AnimationPack.Thriller):Play()
    else
        local p = game.Players.LocalPlayer.Character.Humanoid.Animator:GetPlayingAnimationTracks()
        for q,r in ipairs(p) do
            r:Stop()
        end
    end
end)})

