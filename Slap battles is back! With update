local a = loadstring(game:HttpGet("https://raw.githubusercontent.com/shlexware/Orion/main/source"))()

local b = "Phantom2103"
local c = false

a:MakeNotification({Name = "Welcome to Phantom Hub",Content = "Please enter the key to unlock the script.",Image = "rbxassetid://4483345998",Time = 5})

local d = a:MakeWindow({Name = "Phantom HUB | [UPDATE] SLAP BATTLES üëè",HidePremium = false,SaveConfig = true,ConfigFolder = "PhantomHubConfig",IntroText = "Phantom Hub"})

local e = d:MakeTab({Name = "Key",Icon = "rbxassetid://4483345998",PremiumOnly = false})

e:AddTextbox({Name = "Enter Key",Default = "",TextDisappear = true,Callback = function(f)
    if f == b then
        c = true
        a:MakeNotification({Name = "Success",Content = "Key is correct! Script unlocked.",Image = "rbxassetid://4483345998",Time = 5})
    else
        a:MakeNotification({Name = "Error",Content = "Incorrect key. Please try again.",Image = "rbxassetid://4483345998",Time = 5})
    end
end})

e:AddButton({Name = "Copy Discord Link",Callback = function()
    setclipboard("hxxps://discord[.]com/invite/ZwBTD6kvwa")
    a:MakeNotification({Name = "Copied",Content = "Discord link copied to clipboard.",Image = "rbxassetid://4483345998",Time = 5})
end})

local g = d:MakeTab({Name = "Credits",Icon = "rbxassetid://4483345998",PremiumOnly = false})

g:AddLabel("Owner - Hawk")
g:AddLabel("Phantom Hub")
g:AddLabel("Hello Fix Bugs Report In Discord")

g:AddButton({Name = "Copy Discord Link",Callback = function()
    setclipboard("hxxps://discord[.]com/invite/ZwBTD6kvwa")
    a:MakeNotification({Name = "Copied",Content = "Discord link copied to clipboard.",Image = "rbxassetid://4483345998",Time = 5})
end})

local h = function(i)
    return function(...)
        if c then
            i(...)
        else
            a:MakeNotification({Name = "Locked",Content = "Please enter the correct key to unlock.",Image = "rbxassetid://4483345998",Time = 5})
        end
    end
end

